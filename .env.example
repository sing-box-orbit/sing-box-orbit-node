# API Key for authentication (required in production)
API_KEY=your-secure-api-key-here

# Server host (0.0.0.0 for all interfaces, 127.0.0.1 for localhost only)
HOST=0.0.0.0

# Server port
PORT=3000

# sing-box binary path (use ./bin/sing-box for local dev after running: bun run setup)
SINGBOX_BIN=./bin/sing-box

# sing-box config file path
SINGBOX_CONFIG_PATH=./data/config.json

# sing-box working directory
SINGBOX_WORKING_DIR=./data

# Clash API settings (for sing-box experimental clash_api)
CLASH_API_URL=http://127.0.0.1:9090
CLASH_API_SECRET=

# Log level: debug | info | warn | error
LOG_LEVEL=debug
