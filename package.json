{
  "name": "sing-box-orbit-node",
  "version": "1.12.17",
  "description": "REST API for managing sing-box server",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --compile --bytecode --minify --outfile dist/server",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "setup": "./scripts/download-singbox.sh",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage && bun run scripts/coverage-summary.ts",
    "test:unit": "bun test src/__tests__/unit",
    "test:integration": "bun test src/__tests__/integration"
  },
  "dependencies": {
    "@scalar/api-reference": "^1.44.3",
    "@scalar/hono-api-reference": "^0.9.38",
    "@zhexin/typebox": "npm:@jsr/zhexin__typebox",
    "envalid": "^8.1.1",
    "fets": "^0.8.5",
    "hono": "^4.11.7",
    "hono-rate-limiter": "^0.5.3",
    "typebox": "^1.0.80"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/bun": "^1.3.8",
    "openapi-types": "^12.1.3",
    "typescript": "^5.9.3"
  }
}
