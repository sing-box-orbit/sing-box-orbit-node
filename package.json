{
  "name": "sing-box-orbit-node",
  "version": "0.1.0",
  "description": "REST API for managing sing-box server",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --compile --bytecode --minify --outfile dist/server",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "setup": "./scripts/download-singbox.sh"
  },
  "dependencies": {
    "@scalar/hono-api-reference": "^0.9.32",
    "@sinclair/typebox": "^0.34.47",
    "envalid": "^8.1.1",
    "fets": "^0.8.0",
    "hono": "^4.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bun": "^1.1.0",
    "openapi-types": "^12.1.3",
    "typescript": "^5.6.0"
  }
}
