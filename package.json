{
  "name": "sing-box-orbit-node",
  "version": "0.0.1",
  "description": "REST API for managing sing-box server",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --compile --bytecode --minify --outfile dist/server",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "setup": "./scripts/download-singbox.sh",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage && bun run scripts/coverage-summary.ts",
    "test:unit": "bun test src/__tests__/unit",
    "test:integration": "bun test src/__tests__/integration"
  },
  "dependencies": {
    "@scalar/hono-api-reference": "^0.9.32",
    "@sinclair/typebox": "^0.34.47",
    "envalid": "^8.1.1",
    "fets": "^0.8.5",
    "hono": "^4.11.3",
    "hono-rate-limiter": "^0.5.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.3.5",
    "openapi-types": "^12.1.3",
    "typescript": "^5.9.3"
  }
}
